# Generic
__pycache__

# Ignore everything ...
/*
# ... avoid `*`, it works recursively!
# ... except:

# Screenshots for release
!/.screenshots/

# Docs for release
!/.docs/

# Config for proxychains
!/.proxychains/

# Templates
!/Templates/

# Documentations
!/doc/

# Local share
!/.local/
/.local/*
!/.local/share/
/.local/share/*

# Nautilus scripts
!/.local/share/nautilus/
/.local/share/nautilus/*
!/.local/share/nautilus/scripts/

# Nautilus python extensions
!/.local/share/nautilus-python/

# Applications
!/.local/share/applications/
/.local/share/applications/*.cache
/.local/share/applications/chrome-*
/.local/share/applications/anbox/

## Configs
!/.config/
/.config/*

# Autostart
!/.config/autostart/

# Argos scripts
!/.config/argos/

# User systemd
!/.config/systemd/
/.config/systemd/user/*/

# User bin
!/bin/

# User utils
!/apps/
/apps/*
!/apps/utils/
/apps/utils/*.nb
/apps/utils/zipped/

# Symlink to dictionaries
!/apps/dicts/

# Hardware tweaks
!/apps/hardware/
/apps/hardware/libinput-gestures*
/apps/hardware/*.log

# Shadowsocks
!/apps/shadowsocks/
/apps/shadowsocks/*
!/apps/shadowsocks/*.json
